{
  "name": "thebrain",
  "version": "0.0.1",
  "description": "An alternative frontend for El Bombillo",
  "author": "The Bombillos",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/bitionaire/el-bombillo/issues"
  },
  "homepage": "https://github.com/bitionaire/el-bombillo",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitionaire/el-bombillo"
  },
  "main": "index.js",
  "scripts": {
    "install": "npm install",
    "start": "webpack-dev-server",
    "build:js": "webpack src/js/*.js static/js/main.js",
    "build:css": "lessc src/less/*.less static/css/style.css",
    "postInstall": "cp src/theme.config node_modules/semantic-ui-less && mv node_modules/semantic-ui-less/_site node_modules/semantic-ui-less/site",
    "bump_version": "npm version patch",
    "watch": "webpack-dev-server -w"
  },
  "dependencies": {
    "semantic-ui-less": "^1.12.3"
  },
  "devDependencies": {
    "less": "^2.5.1",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  }
}
